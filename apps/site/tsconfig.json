{
  "extends": "tsconfig/nextjs.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": false,
    "baseUrl": ".",
    "downlevelIteration": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "../../packages/shared/src/filter-data/parsers.ts",
    "app/(front)/(docs)/privacy/page.mdx"
  ],
  "exclude": ["node_modules", "supabase"]
}
